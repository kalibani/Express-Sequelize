<!DOCTYPE html>
<html>
  <%- include partial/head %>
  <body>
    <h1>Edit Data Teacher</h1>
    <a href="/teacher">Show Data Teacher</a>
    <form class="" action="/teacher/edit/<%= data.id%>" method="POST">
        <input type="text" name="first_name" value="<%= data.first_name%>" placeholder="first_name">
        <input type="text" name="last_name" value="<%= data.last_name%>" placeholder="last_name">
        <input type="email" name="email" value="<%= data.email%>" placeholder="email">
        <select name="SubjectId">
          <% data2.forEach(row => { %>
          <option value="<%= row.id%>"><%= row.subject_name %></option>
        <% }) %>
        </select>
        <input type="submit" value="SAVE">
    </form>
  </body>
</html>
